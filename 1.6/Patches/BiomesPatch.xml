<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- Temperate Biomes -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "TemperateForest"]/wildPlants</xpath>
		<value>
			<VCE_AppleTree>0.01</VCE_AppleTree>
			<VCE_BearBerryBush>0.01</VCE_BearBerryBush>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "AridShrubland"]/wildPlants</xpath>
		<value>
			<VCE_PeachTree>0.01</VCE_PeachTree>
			<VCE_Opuntia>0.01</VCE_Opuntia>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "BorealForest"]/wildPlants</xpath>
