			<VCE_BearBerryBush>0.01</VCE_BearBerryBush>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "TemperateSwamp"]/wildPlants</xpath>
		<value>
			<VCE_PlumTree>0.01</VCE_PlumTree>
			<VCE_PearTree>0.01</VCE_PearTree>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants</xpath>
		<value>
			<VCE_PlumTree>0.01</VCE_PlumTree>
			<VCE_PearTree>0.01</VCE_PearTree>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "Desert"]/wildPlants</xpath>
		<value>
			<VCE_PeachTree>0.01</VCE_PeachTree>
			<VCE_Opuntia>0.01</VCE_Opuntia>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "Tundra"]/wildPlants</xpath>
		<value>
			<VCE_CherryTree>0.01</VCE_CherryTree>
			<VCE_BearBerryBush>0.01</VCE_BearBerryBush>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "TropicalRainforest"]/wildPlants</xpath>
		<value>
			<VCE_BananaTree>0.01</VCE_BananaTree>
			<VCE_OrangeTree>0.01</VCE_OrangeTree>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "TropicalSwamp"]/wildPlants</xpath>
		<value>
			<VCE_BananaTree>0.01</VCE_BananaTree>
			<VCE_OrangeTree>0.01</VCE_OrangeTree>
		</value>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Odyssey</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Grasslands"]/wildPlants</xpath>
					<value>
						<VCE_AppleTree>0.01</VCE_AppleTree>
						<VCE_BearBerryBush>0.01</VCE_BearBerryBush>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "LavaField"]/wildPlants</xpath>
					<value>
						<VCE_PeachTree>0.01</VCE_PeachTree>
						<VCE_Opuntia>0.01</VCE_Opuntia>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Scarlands"]/wildPlants</xpath>
					<value>
						<VCE_PeachTree>0.01</VCE_PeachTree>
						<VCE_Opuntia>0.01</VCE_Opuntia>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "GlacialPlain"]/wildPlants</xpath>
					<value>
						<VCE_CherryTree>0.01</VCE_CherryTree>
						<VCE_BearBerryBush>0.01</VCE_BearBerryBush>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>